 RowBox[{
  RowBox[{"SetSlicing", "[", 
   RowBox[{"g", ",", " ", "n", ",", " ", "h", ",", " ", "cd", ",", " ", 
    RowBox[{"{", 
     RowBox[{"\"\<|\>\"", ",", " ", "\"\<D\>\""}], "}"}], ",", " ", 
