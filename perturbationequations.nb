  RowBox[{"(*", 
   RowBox[{"Useful", " ", "function"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"DefConstantSymbol", "[", "k", "]"}], " ", 
  RowBox[{"(*", 
   RowBox[{
   "This", " ", "will", " ", "be", " ", "the", " ", "wavenumber", " ", "k", 
